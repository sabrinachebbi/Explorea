<form action="{{ path('userProfile_delete', {'id': userProfile.id}) }}" method="post" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer ce profil ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ userProfile.id) }}">
    <button type="submit" class="btn btn-danger">Supprimer</button>
</form>
